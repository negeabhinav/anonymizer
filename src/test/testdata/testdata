CREATE TABLE person (
    ID INT PRIMARY KEY,
    name VARCHAR(255),
    age INT,
    phone_number VARCHAR(20),
    full_address VARCHAR(255),
    Adhaar_CARD VARCHAR(12),
	PAN VARCHAR2(50)
);

